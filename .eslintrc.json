{
  "root": true,
  "env": {
    "es2020": true
  },
  "extends": ["trybe-backend", "@trybe/eslint-config-frontend", "plugin:cypress/recommended"],
  "rules": {
    "no-tabs": "off",
    "sonarjs/no-duplicate-string": [
      "error",
      5
    ],
    "sonarjs/prefer-object-literal": "off",
    "mocha/no-setup-in-describe": "off"
  }
}